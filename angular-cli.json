{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "ng2-markdown-to-html"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "app/markdown-demo/demo.cpp",
        "app/markdown-demo/demo.html",
        "app/markdown-demo/demo.java",
        "app/markdown-demo/demo.md",
        "app/markdown-demo/demo.py"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "markdown",
      "mobile": false,
      "styles": [
        "styles.css",
        "../node_modules/prismjs/themes/prism.css"
      ],
      "scripts": [],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
